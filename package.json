{
  "name": "typed-translator",
  "version": "0.1.0",
  "description": "Strictly typed i18n library for TypeScript",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rm -rf ./dist ./result && mkdir ./result",
    "build": "npm run clean && tsc",
    "cmd": "npm run build && node ./dist/cmd/cmd.js",
    "cmd:example": "npm run cmd ./example/resources ./result/example.d.ts",
    "test": "jest"
  },
  "bin": {
    "typed-translator": "./dist/cmd/cmd.js"
  },
  "keywords": [
    "i18n",
    "translate",
    "TypeScript",
    "compile time"
  ],
  "author": "mmktomato",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mmktomato/typed-translator"
  },
  "bugs": {
    "url": "https://github.com/mmktomato/typed-translator/issues"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.11.7",
    "jest": "^24.9.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "intl-messageformat-parser": "^3.2.2"
  },
  "engines": {
    "node": ">=8"
  }
}
